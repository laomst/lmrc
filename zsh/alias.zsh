#!/bin/zsh
# 命令别名配置

# ls 别名
alias lsa="ls -a"
alias ll="ls -alF"
alias la="ls -A"
alias l="ls -CF"

# Python 别名
alias python='python3'
alias pip='pip3'

# 常用快捷方式
alias ..='cd ..'
alias ...='cd ../..'
alias ~='cd ~'

# Git 别名
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gp='git push'
